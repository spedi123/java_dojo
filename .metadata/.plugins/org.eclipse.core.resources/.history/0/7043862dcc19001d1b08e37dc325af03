package com.spedi123.languages.models;


@Entity
@Table(name = "languages")
public class Language {
	// ========== Primary Key ==========
	@Id
		@GeneratedValue(strategy = GenerationType.IDENTITY)
		private Long id;

	// ========== Member Variables ==========

	@NotNull
	    @Size(min = 2, max = 128, message = "Dog name must be at least 2 characters. CUSTOM")
	    private String name;
}
